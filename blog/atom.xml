---
layout: nil
author: hleinone
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Javanto Blog</title>
	<link href="http://javanto.com/blog/atom.xml" rel="self" />
	<link href="http://javanto.com/blog/" />
	<id>http://javanto.com/blog/</id>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	{% assign author = site.authors[page.author] %}
	<author>
		<name>{{ author.display_name }}</name>
		<email>{{ author.email }}</email>
	</author>

	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://javanto.com{{ post.url }}/" />
		<id>http://javanto.com{{ post.id }}</id>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}
</feed>
